function E2_Conditions takes nothing returns boolean
    if IsUnitType(GetDyingUnit(), UNIT_TYPE_HERO) == true then
        return false
    elseif IsUnitAlly(GetDyingUnit(), GetOwningPlayer(GetKillingUnit())) == true then
        return false
    elseif GetPlayerId(GetOwningPlayer(GetDyingUnit())) < 10 then
        return false
    endif
    return true
endfunction

function E2Con takes nothing returns boolean
    if udg_hero[GetConvertedPlayerId(GetOwningPlayer(GetFilterUnit()))] == GetFilterUnit() then
        return true
    endif
    return false
endfunction


function E2_Actions takes nothing returns nothing
local real exp= 0.00
local real exp2= 0
local unit u= GetDyingUnit()
local unit k= GetKillingUnit()
local integer ulevel= GetUnitLevel(u)
local unit gu= null
local integer L= 0
local real M= 0
local integer i= 0
local real range= 1000.00
local integer g= s__party_create()
call s__SU_DTem(u , k)
if s__SU_DUB(u) == true then
set u=null
set k=null
call s__party_deallocate((g)) // INLINED!!
return
endif
set L=GetHeroLevel(udg_hero[GetPlayerId(GetOwningPlayer(k)) + 1]) - ulevel
//60Îû©Ï∞®Ïù¥Í∞Ä ÎÇòÎ©¥ Ìï®Ïàò Ï¢ÖÎ£å
if L > 60 then
set u=null
set k=null
call s__party_deallocate((g)) // INLINED!!
return
endif
if L > 20 then
set exp2=s__SU_ReturnE2(u)
set exp2=exp2 * 0.35
else
set exp2=s__SU_ReturnE2(u)
