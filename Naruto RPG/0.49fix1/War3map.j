function statsA takes unit u returns integer
local integer STR= GetHeroStr(u, true)
local integer AGI= GetHeroAgi(u, true)
local integer INT= GetHeroInt(u, true)
local integer i= 0
local integer x= 0
if STR > AGI then
set i=1
set x=STR
else
set i=2
set x=AGI
endif
if INT > x then
set i=3
set x=INT
endif
if i == 1 then
return GetHeroStr(u, true)
elseif i == 2 then
return GetHeroAgi(u, true)
elseif i == 3 then
return GetHeroInt(u, true)
endif
return 0
endfunction


