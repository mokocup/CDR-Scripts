function GetInventoryIndex takes unit whichUnit,integer itemId returns integer
    local integer index= 0
    local item indexItem
    local integer count= 0
    loop
        set indexItem=UnitItemInSlot(whichUnit, index)
        if ( indexItem != null ) and ( GetItemTypeId(indexItem) == itemId ) then
            set count=count + 1
        endif
        set index=index + 1
        exitwhen index >= bj_MAX_INVENTORY
    endloop
    return count
endfunction

function Recipe takes unit a,integer b,integer c,integer d,integer e,integer f,integer g,integer h,integer i,integer j,integer k,integer l,integer n,integer m returns nothing
local integer index= 0
local integer array ID
local integer Count= 0
set ID[0]=b
set ID[1]=d
set ID[2]=f
set ID[3]=h
set ID[4]=j
set ID[5]=l
set ID[6]=c
set ID[7]=e
set ID[8]=g
set ID[9]=i
set ID[10]=k
set ID[11]=n
loop
   if ( GetInventoryIndex(a , ID[index]) >= ID[index + 6] ) then
   set Count=Count + 1
   endif
        set index=index + 1
        exitwhen index >= bj_MAX_INVENTORY
endloop
if ( Count >= 6 ) then
set index=0
loop
call RemoveItem(GetItemOfTypeFromUnitBJ(a, ID[index]))
    set index=index + 1
    exitwhen index >= bj_MAX_INVENTORY
endloop
call UnitAddItemByIdSwapped(m, a)
endif
endfunction

