return
elseif i == 1 then
set M=1.00
elseif i == 2 then
set M=1.20
elseif i == 3 then
set M=1.40
elseif i == 4 then
set M=1.70
elseif i >= 5 then
set M=2.00
endif
set exp=exp2 * M
set exp=exp / I2R(i)
            loop
                set gu=FirstOfGroup((s__party_G[(g)])) // INLINED!!
                exitwhen gu == null
                if IsUnitType(gu, UNIT_TYPE_HERO) then
                if GetHeroLevel(gu) - ulevel < 60 then
