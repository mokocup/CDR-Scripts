function Trig_Save_Conditions takes nothing returns boolean
    if ( not ( udg_codeSavingNow[GetConvertedPlayerId(GetTriggerPlayer())] == false ) ) then
        return false
    endif
    if ( not ( udg_BooleanTest == false ) ) then
        return false
    endif
    if ( not ( StringCase(SubStringBJ(GetEventPlayerChatString(), 1, 5), false) == "-save" ) ) then
        return false
    endif
    return true
endfunction

function Trig_Save_Func009Func001C takes nothing returns boolean
    if ( not ( ( udg_saveTick[GetConvertedPlayerId(GetTriggerPlayer())] + 1 ) > 1000000000 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Save_Func009C takes nothing returns boolean
    if ( not ( udg_hero[GetConvertedPlayerId(GetTriggerPlayer())] != null ) ) then
        return false
    endif
    return true
endfunction

function Trig_Save_Func016Func001C takes nothing returns boolean
    if ( not ( StringCase(GetPlayerName(GetTriggerPlayer()), false) != StringCase(udg_TestID, false) ) ) then
        return false
    endif
    return true
endfunction

