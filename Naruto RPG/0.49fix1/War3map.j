            local integer this
            local unit u= GetFilterUnit()
            if GetUnitAbilityLevel(u, ModifiedEvents__ABILITIES_UNIT_INDEXER) == 0 then
                if (LoadInteger(ModifiedEvents__HASH, 0, GetHandleId((u)))) != 0 then // INLINED!!
                    set this=(LoadInteger(ModifiedEvents__HASH, 0, GetHandleId((u)))) // INLINED!!
                    call h__DestroyTrigger(s__ModifiedEvents__UnitAttach_tDamaged[this])
                    call RemoveSavedInteger(ModifiedEvents__HASH, 0, GetHandleId((u))) // INLINED!!
                    call s__ModifiedEvents__Allocator_Return(this)
                endif
            endif
            set u=null
            return true
        endfunction
        
