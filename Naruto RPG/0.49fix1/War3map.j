    endif
    return true
endfunction

function Trig_Save_Actions takes nothing returns nothing
    local unit hero= udg_hero[GetConvertedPlayerId(GetTriggerPlayer())]
    local unit bag= udg_bag[GetConvertedPlayerId(GetTriggerPlayer())]
    // ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
    // ÏÑ∏Ïù¥Î∏å Îç∞Ïù¥ÌÑ∞Î•º ÏÑ§Ï†ïÌï©ÎãàÎã§.
    call SaveFaster(hero , GetConvertedPlayerId(GetTriggerPlayer()))
    set udg_data[udg_saveDataGold]=GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_GOLD)
    set udg_data[udg_saveDataLumber]=GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_LUMBER)
    set udg_data[udg_saveDataHeroType]=LoadIntegerBJ(( GetUnitTypeId(hero) ), 0, udg_saveHeroHash)
    if ( Trig_Save_Func009C() ) then
        if ( Trig_Save_Func009Func001C() ) then
            set udg_saveTick[GetConvertedPlayerId(GetTriggerPlayer())]=udg_saveTick[GetConvertedPlayerId(GetTriggerPlayer())]
        else
            set udg_saveTick[GetConvertedPlayerId(GetTriggerPlayer())]=( udg_saveTick[GetConvertedPlayerId(GetTriggerPlayer())] + 1 )
        endif
    else
    endif
