        call SetUnitX(dummy, GetUnitX(pUnit))
        call SetUnitY(dummy, GetUnitY(pUnit))
        call SetUnitFlyHeight(dummy, GetUnitFlyHeight(pUnit), 0)
        set pUnit=null
