local location CricleMove= GetUnitLoc(a)
set OriginalAngle=AngleBetweenPoints(CircleLoc, CricleMove)
call RemoveLocation(CricleMove)
call SaveUnitHandle(udg_Hashtable, GetHandleId(t), StringHash("Circle"), a)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("Distance"), c)
if ( g == true ) then
else
set A=- ( A )
endif
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("Angle"), A)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("LocX"), LocX)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("LocY"), LocY)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("Cease"), number)
set CricleMove=PolarProjectionBJ(CircleLoc, c, OriginalAngle + A)
call SetUnitX(a, GetLocationX(CricleMove))
call SetUnitY(a, GetLocationY(CricleMove))
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("CeaseNumber"), 1)
call TimerStart(t, e, true, function Circle1)
