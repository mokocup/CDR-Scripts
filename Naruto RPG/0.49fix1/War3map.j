    function MixSys__ChkRecipe takes integer rIdx,unit u returns nothing
        local integer rnum= s__sjRecipe_itn[MixSys__RList[rIdx]]
        local integer mIdx= s__sjRecipe_startN[MixSys__RList[rIdx]]
        local integer array nList
        local integer tnum
        local integer i= 0
        local integer j
        local integer usr= GetPlayerId(GetOwningPlayer(u)) + 1
        local boolean fl= true
        local item tmp
        if GetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_GOLD) < s__sjRecipe_gold[MixSys__RList[rIdx]] then
            set tmp=null
            set s__MixSys__accF[usr]= false
            return
        endif
        if GetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_LUMBER) < s__sjRecipe_lumber[MixSys__RList[rIdx]] then
            set tmp=null
            set s__MixSys__accF[usr]= false
            return
        endif
        loop
            set j=0
            loop
                set tmp=UnitItemInSlot(u, j)
                if GetItemTypeId(tmp) == MixSys__MList[mIdx + i] then
                    set tnum=GetItemCharges(tmp)
                    if tnum == 0 then
                        set tnum=1
                    endif
                    set nList[i]=nList[i] + tnum
                endif
                set j=j + 1
                exitwhen j > 5
            endloop
            set i=i + 1
            exitwhen i >= rnum
        endloop
        set i=0
        loop
            if nList[i] < MixSys__MnList[mIdx + i] then
                set fl=false
            else
                set nList[i]=MixSys__MnList[mIdx + i]
            endif
            set i=i + 1
            exitwhen i >= rnum or not fl
        endloop
        set tmp=null
        set s__MixSys__accF[usr]= fl
    endfunction
    
