            set s__ModifiedEvents__UnitAttach_tDamaged[this]=CreateTrigger()
            call TriggerRegisterUnitEvent(s__ModifiedEvents__UnitAttach_tDamaged[this], u, EVENT_UNIT_DAMAGED)
            call TriggerAddCondition(s__ModifiedEvents__UnitAttach_tDamaged[this], Condition(function s__ModifiedEvents__UnitAttach_UnitDamaged))
            
            set u=null
            return true
        endfunction
        
