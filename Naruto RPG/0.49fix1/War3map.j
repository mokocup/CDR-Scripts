function jump1 takes nothing returns nothing
local timer t= GetExpiredTimer()
local unit a= LoadUnitHandle(udg_Hashtable, GetHandleId(t), StringHash("unit"))
local real multiple= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("multiple"))
local real max= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("max"))
local real speed= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("speed"))
local real distance= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("distance"))
local real min= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("min"))
local real height= LoadReal(udg_Hashtable, GetHandleId(t), StringHashBJ("height"))
local real end= 0
if ( min * 2 >= distance ) then
call FlushChildHashtable(udg_Hashtable, GetHandleId(t))
call DestroyTimer(t)
call SetUnitFlyHeightBJ(a, height, 0)
else
set min=min + speed
     if ( ( height + - ( Pow(min, 2.00) / distance ) * multiple ) + max <= height ) then
     call SetUnitFlyHeightBJ(a, height, 0)
     else
     call SetUnitFlyHeightBJ(a, height + - ( ( Pow(min, 2.00) / distance ) * multiple ) + max, 0)
     endif
     if ( IsUnitAliveBJ(a) == false ) then
     call FlushChildHashtable(udg_Hashtable, GetHandleId(t))
     call DestroyTimer(t)
     endif
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("min"), min)
endif
set a=null
set t=null
endfunction

function jump takes unit u,real d,real m,real g returns nothing
local timer t= CreateTimer()
local real height= GetUnitFlyHeight(u)
local real min= ( 300 / 2 ) * - 1
local real r= ( min * min ) / 300
local real multiple= m / r
local real w
set w=d / g
set w=300 / w
set d=w
call SaveUnitHandle(udg_Hashtable, GetHandleId(t), StringHash("unit"), u)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("height"), height)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("max"), m)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("multiple"), multiple)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("distance"), 300)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("speed"), d)
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("min"), min)
call TimerStart(t, g, true, function jump1)
set t=null
endfunction

