function PlusStat takes integer mode,unit u,integer input returns nothing
local integer i= 16
local integer x
local integer num= mode * 17
set x=input
    if x < 0 then
        return
    elseif x > R2I(Pow(2, 17) - 1) then
        set x=R2I(Pow(2, 17) - 1)
    endif
        loop
            if x >= udg_XValue[i] then
                set x=x - udg_XValue[i]
                call UnitAddAbility(u, udg_ADDSTATE[i + num])
                call UnitMakeAbilityPermanent(u, true, udg_ADDSTATE[i + num])
            else
                call UnitMakeAbilityPermanent(u, false, udg_ADDSTATE[i + num])
                call UnitRemoveAbility(u, udg_ADDSTATE[i + num])
            endif
            set i=i - 1
            exitwhen i < 0
        endloop
endfunction

