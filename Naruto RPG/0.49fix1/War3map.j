local real Distance= LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Distance"))
local real Angle= LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Angle"))
local real OriginalAngle
local location CircleLoc= Location(LocX, LocY)
local location CircleMove= GetUnitLoc(CircleUnit)
set OriginalAngle=AngleBetweenPoints(CircleLoc, CircleMove)
call RemoveLocation(CircleMove)
set CircleMove=PolarProjectionBJ(CircleLoc, Distance, OriginalAngle + Angle)
  if ( CeaseNumber >= Cease ) then
  call FlushChildHashtable(udg_Hashtable, GetHandleId(t))
  call DestroyTimer(t)
  call RemoveUnit(CircleUnit)
  else
  set CeaseNumber=CeaseNumber + 1
  call SetUnitX(CircleUnit, GetLocationX(CircleMove))
  call SetUnitY(CircleUnit, GetLocationY(CircleMove))
  call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("CeaseNumber"), CeaseNumber)
  endif
call RemoveLocation(CircleMove)
call RemoveLocation(CircleLoc)
set CircleUnit=null
set t=null
set CircleMove=null
set CircleLoc=null
endfunction

function Circle takes unit a,real f,location b,real c,real d,real e,boolean g returns nothing
local real LocX= GetLocationX(b)
local real LocY= GetLocationY(b)
local timer t= CreateTimer()
local real OriginalAngle
local real number= d / e
local real A= 360 / ( c / f )
