    local integer bj_Loopindex= 19
    if life != 0 then
    if life > 0 then
    set y[19]=262144
    loop
    exitwhen x < 1
    set y[x]=y[x + 1] / 2
    set x=x - 1
    endloop
    set x=life
    loop
    exitwhen x == 0
    if x > 0 then
    if x >= y[bj_Loopindex] then
        set x=x - y[bj_Loopindex]
        call UnitAddAbility(u, 'A06I')
        call SetUnitAbilityLevel(u, 'A06I', 1 + bj_Loopindex)
        call UnitRemoveAbility(u, 'A06I')
    endif
    endif
    set bj_Loopindex=bj_Loopindex - 1
    endloop
else
    set y[19]=- 262144
    loop
    exitwhen x < 1
    set y[x]=y[x + 1] / 2
    set x=x - 1
    endloop
    set x=life
    loop
    exitwhen x == 0
    if x < 0 then
    if x <= y[bj_Loopindex] then
        set x=x - y[bj_Loopindex]
        call UnitAddAbility(u, 'A06H')
        call SetUnitAbilityLevel(u, 'A06H', 1 + bj_Loopindex)
        call UnitRemoveAbility(u, 'A06H')
    endif
    endif
    set bj_Loopindex=bj_Loopindex - 1
    endloop
endif
endif
endfunction

