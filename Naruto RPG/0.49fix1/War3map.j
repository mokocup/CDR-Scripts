            local rect r= GetWorldBounds()
            local region rectRegion= CreateRegion()
            local integer i= 0
            call GroupEnumUnitsInRect(g, r, Condition(function s__ModifiedEvents__UnitAttach_onUnitIndex))
            call RegionAddRect(rectRegion, r)
            call TriggerRegisterEnterRegion(s__ModifiedEvents__UnitAttach_onUnitIndexer, rectRegion, Condition(function s__ModifiedEvents__UnitAttach_onUnitIndex))
            loop
                call TriggerRegisterPlayerUnitEvent(s__ModifiedEvents__UnitAttach_onUnitDeindexer, Player(i), EVENT_PLAYER_UNIT_ISSUED_ORDER, Condition(function s__ModifiedEvents__UnitAttach_onUnitDeindex))
