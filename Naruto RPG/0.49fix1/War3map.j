            set fl=true
        endif
        if fl then
            set tmp=CreateItem(s__sjRecipe_it[rp], GetUnitX(u), GetUnitY(u))
            call UnitAddItem(u, tmp)
            if s__sjRecipe_rn[rp] > 1 then
                if GetItemCharges(tmp) == 0 then
                    set i=1
                    loop
                        set tmp=CreateItem(s__sjRecipe_it[rp], GetUnitX(u), GetUnitY(u))
                        call SetItemPlayer(tmp, GetOwningPlayer(u), false)
                        call UnitAddItem(u, tmp)
                        set i=i + 1
                        exitwhen i >= s__sjRecipe_rn[rp]
                    endloop
                else
                    call SetItemCharges(tmp, s__sjRecipe_rn[rp])
                endif
            endif
        else
            set falseF=true
        endif
        if s__MixSys__tempProb[usr] != 0. then
            set s__MixSys__tempProb[usr]= 0.
        endif
        set s__MixSys__nowResetting[usr]= MixSys__ResetRecipeTempProb(usr)
        set MixSys__lastExpectedItem=s__sjRecipe_it[rp]
        if falseF then
            if MixSys__trgFail != null then
                call TriggerExecute(MixSys__trgFail)
            endif
        else
            if MixSys__trgSuccess != null then
                call TriggerExecute(MixSys__trgSuccess)
            endif
        endif
        set tmp=null
