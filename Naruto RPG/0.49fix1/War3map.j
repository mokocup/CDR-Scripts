function poisonattack takes nothing returns nothing
local timer t= GetExpiredTimer()
local unit source= LoadUnitHandle(udg_Hashtable, GetHandleId(t), StringHash("Source"))
local unit target=LoadUnitHandle(udg_Hashtable, GetHandleId(t), StringHash("Target"))
local real damage= LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Damage"))
local real frequency=LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Frequency"))
local real number= LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Number"))
local string impact= LoadStr(udg_Hashtable, GetHandleId(t), StringHash("impact"))
local string sticking= LoadStr(udg_Hashtable, GetHandleId(t), StringHash("Sticking"))
local real DestroyE= LoadReal(udg_Hashtable, GetHandleId(t), StringHash("Destroy"))
if ( number >= frequency ) then
     call FlushChildHashtable(udg_Hashtable, GetHandleId(t))
     call DestroyTimer(t)
else
     if ( IsUnitAliveBJ(target) == true ) then
     call UnitDamageTargetBJ(source, target, damage, ATTACK_TYPE_MELEE, DAMAGE_TYPE_NORMAL)
      call AddSpecialEffectTargetUnitBJ(sticking, target, impact)
      call EDestroy(GetLastCreatedEffectBJ() , DestroyE)
 
     else
     call FlushChildHashtable(udg_Hashtable, GetHandleId(t))
     call DestroyTimer(t)
     endif
set number=number + 1
call SaveReal(udg_Hashtable, GetHandleId(t), StringHash("Number"), number)
endif
set target=null
set source=null
set t=null
endfunction

