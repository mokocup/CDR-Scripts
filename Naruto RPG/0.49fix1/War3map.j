function SaveFaster takes unit u,integer i returns nothing
local integer id= GetPlayerId(GetOwningPlayer(u))
    set udg_data[udg_saveDataHeroStr]=HeroSTR[id]
    set udg_data[udg_saveDataHeroAgi]=HeroAGI[id]
    set udg_data[udg_saveDataHeroInt]=HeroINT[id]
    set udg_data[udg_saveDataHeroItem1]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 0)) ))
    set udg_data[udg_saveDataHeroItem2]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 1)) ))
    set udg_data[udg_saveDataHeroItem3]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 2)) ))
    set udg_data[udg_saveDataHeroItem4]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 3)) ))
    set udg_data[udg_saveDataHeroItem5]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 4)) ))
    set udg_data[udg_saveDataHeroItem6]=LoadInteger(udg_saveItemHash, 0, ( GetItemTypeId(UnitItemInSlot(u, 5)) ))
    set udg_data[udg_saveDataHeroCharge1]=GetItemCharges(UnitItemInSlot(u, 0))
    set udg_data[udg_saveDataHeroCharge2]=GetItemCharges(UnitItemInSlot(u, 1))
