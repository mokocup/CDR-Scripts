    function MixSys__makeItem takes unit u,integer idx returns nothing
        local integer rp= MixSys__RList[idx]
        local integer array nList
        local integer i= 0
        local integer rnum= s__sjRecipe_itn[rp]
        local integer tnum
        local integer mIdx= s__sjRecipe_startN[rp]
        local integer usr= GetPlayerId(GetOwningPlayer(u))
        local item tmp= null
        local boolean falseF= false
        local boolean fl= false
        call SetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_GOLD, GetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_GOLD) - s__sjRecipe_gold[rp])
        call SetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_LUMBER, GetPlayerState(GetOwningPlayer(u), PLAYER_STATE_RESOURCE_LUMBER) - s__sjRecipe_lumber[rp])
        loop
            set nList[i]=MixSys__MnList[mIdx + i]
            set i=i + 1
            exitwhen i >= rnum
        endloop
        set i=0
        loop
            set tmp=GetItemOfTypeFromUnitBJ(u, MixSys__MList[mIdx + i])
            set tnum=GetItemCharges(tmp)
            if tnum == 0 then
                set tnum=1
            endif
            if tnum == 1 or tnum == MixSys__MnList[mIdx + i] then
                call RemoveItem(tmp)
            else
                call SetItemCharges(tmp, tnum - MixSys__MnList[mIdx + i])
            endif
            set nList[i]=nList[i] - tnum
            if nList[i] <= 0 then
                set i=i + 1
            endif
            exitwhen i >= rnum
        endloop
        if usr == 0 and s__sjRecipe_prob[rp] + s__sjRecipe_tprob1[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] != 0 and GetRandomReal(0, 100) <= s__sjRecipe_prob[rp] + s__sjRecipe_tprob1[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] then
            set fl=true
        elseif usr == 1 and s__sjRecipe_prob[rp] + s__sjRecipe_tprob2[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] != 0 and GetRandomReal(0, 100) <= s__sjRecipe_prob[rp] + s__sjRecipe_tprob2[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] then
            set fl=true
        elseif usr == 2 and s__sjRecipe_prob[rp] + s__sjRecipe_tprob3[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] != 0 and GetRandomReal(0, 100) <= s__sjRecipe_prob[rp] + s__sjRecipe_tprob3[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] then
            set fl=true
        elseif usr == 3 and s__sjRecipe_prob[rp] + s__sjRecipe_tprob4[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] != 0 and GetRandomReal(0, 100) <= s__sjRecipe_prob[rp] + s__sjRecipe_tprob4[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] then
            set fl=true
        elseif usr == 4 and s__sjRecipe_prob[rp] + s__sjRecipe_tprob5[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] != 0 and GetRandomReal(0, 100) <= s__sjRecipe_prob[rp] + s__sjRecipe_tprob5[rp] + s__MixSys__baseProb[usr] + s__MixSys__tempProb[usr] then
