    function PSAlocDetector___OnParticleCreate takes nothing returns nothing
        local integer p= GetLastCreatedParticleHH()
        local unit pUnit= GetParticleUnitHH(p)
        local real x= GetUnitX(pUnit)
        local real y= GetUnitY(pUnit)
        local unit dummy
        if ( GetUnitAbilityLevel(pUnit, PSAlocDetector___ABILITY_RAWCODE) > 0 ) then
            set dummy=CreateUnit(GetOwningPlayer(pUnit), PSAlocDetector___DUMMY_RAWCODE, x, y, 0)
            call SetUnitX(dummy, x)
            call SetUnitY(dummy, y)
            call SetUnitFlyHeight(dummy, GetUnitFlyHeight(pUnit), 0)
            call SetUnitUserData(dummy, p)
            call SaveUnitHandle(PSAlocDetector___hash, 0, GetHandleId(pUnit), dummy)
            call GroupAddUnit(PSAlocDetector___detectorGroup, dummy)
        endif
        set pUnit=null
        set dummy=null
    endfunction

    function PSAlocDetector___OnParticleDestroy takes nothing returns nothing
        local integer p= GetTriggerParticleHH()
        local unit pUnit= GetParticleUnitHH(p)
        local unit dummy
        if ( GetUnitAbilityLevel(pUnit, PSAlocDetector___ABILITY_RAWCODE) > 0 ) then
            set dummy=LoadUnitHandle(PSAlocDetector___hash, 0, GetHandleId(pUnit))
            call RemoveSavedHandle(PSAlocDetector___hash, 0, GetHandleId(pUnit))
            call GroupRemoveUnit(PSAlocDetector___detectorGroup, dummy)
            call RemoveUnit(dummy)
        endif
        set pUnit=null
        set dummy=null
    endfunction
    
    function PSAlocDetector___DetectorGroupCallback takes nothing returns nothing
