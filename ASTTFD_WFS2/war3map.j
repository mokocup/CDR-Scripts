function PD takes location qD,real QD,real sD returns location
return Location(GetLocationX(qD)+QD*Cos(sD*bj_DEGTORAD),GetLocationY(qD)+QD*Sin(sD*bj_DEGTORAD))
endfunction
function SD takes nothing returns boolean
local real dx=GetDestructableX(GetFilterDestructable())-KD
local real dy=GetDestructableY(GetFilterDestructable())-lD
return(dx*dx+dy*dy<=bj_enumDestructableRadius)
endfunction
function tD takes real TD,location uD,code UD returns nothing
local rect r
if(TD>=0)then
set KD=GetLocationX(uD)
set lD=GetLocationY(uD)
set bj_enumDestructableRadius=TD*TD
set r=Rect(KD-TD,lD-TD,KD+TD,lD+TD)
call EnumDestructablesInRect(r,filterEnumDestructablesInCircleBJ,UD)
call RemoveRect(r)
set r=null
endif
endfunction
function wD takes rect r returns group
set LD=CreateGroup()
call GroupEnumUnitsInRect(LD,r,MD)
return LD
endfunction
function WD takes rect r,player yD returns group
set LD=CreateGroup()
set bj_groupEnumOwningPlayer=yD
call GroupEnumUnitsInRect(LD,r,filterGetUnitsInRectOfPlayer)
return LD
endfunction
function YD takes real TD,location zD,boolexpr ZD returns group
set LD=CreateGroup()
call GroupEnumUnitsInRangeOfLoc(LD,zD,TD,ZD)
return LD
endfunction
function vf takes real TD,location zD returns group
set LD=CreateGroup()
call GroupEnumUnitsInRangeOfLoc(LD,zD,TD,MD)
return LD
endfunction
function ef takes integer xf returns group
set LD=CreateGroup()
call GroupEnumUnitsOfType(LD,UnitId2String(xf),MD)
return LD
endfunction
function of takes player yD,boolexpr ZD returns group
set LD=CreateGroup()
call GroupEnumUnitsOfPlayer(LD,yD,ZD)
call DestroyBoolExpr(ZD)
return LD
endfunction
function rf takes player yD returns group
set LD=CreateGroup()
call GroupEnumUnitsOfPlayer(LD,yD,MD)
return LD
endfunction
function af takes player yD,integer xf returns group
set LD=CreateGroup()
set bj_groupEnumTypeId=xf
call GroupEnumUnitsOfPlayer(LD,yD,filterGetUnitsOfPlayerAndTypeId)
