function log takes real y,real base returns real
local real x
local real factor=1.
local real logy=.0
local real sign=1.
if(y<.0)then
return .0
endif
if(y<1.)then
set y=1./y
set sign=-1.
endif
loop
exitwhen y<1.0001
if(y>base)then
set y=y/base
set logy=logy+factor
else
set base=SquareRoot(base)
set factor=factor/2.
endif
endloop
return sign*logy
endfunction
