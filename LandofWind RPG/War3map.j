function Savecode___scommhash takes string s returns integer
local integer array count
local integer i=0
local integer len=StringLength(s)
local integer x
set s=StringCase(s,true)
loop
exitwhen i>=len
set x=Savecode___player_chartoi(SubString(s,i,i+1))
set count[x]=count[x]+1
set i=i+1
endloop
set i=0
set len=Savecode___player_charsetlen()
set x=0
loop
exitwhen i>=len
set x=count[i]*count[i]*i+count[i]*x+x+199
set i=i+1
endloop
if x<0 then
set x=-x
endif
return x
endfunction
function Savecode___modb takes integer x returns integer
if x>=Savecode___BASE()then
return x-Savecode___BASE()
elseif x<0 then
return x+Savecode___BASE()
else
return x
endif
endfunction
