function s__Savecode_Pad takes integer this returns nothing
local integer cur=s__BigNum_list[s__Savecode_bignum[this]]
local integer prev
local integer maxlen=R2I(1.+s__Savecode_Length(this))
loop
exitwhen cur==0
set prev=cur
set cur=s__BigNum_l_next[cur]
set maxlen=maxlen-1
endloop
loop
exitwhen maxlen<=0
set s__BigNum_l_next[prev]=s__BigNum_l_create()
set prev=s__BigNum_l_next[prev]
set maxlen=maxlen-1
endloop
endfunction
function s__Savecode_ToString takes integer this returns string
local integer cur=s__BigNum_list[s__Savecode_bignum[this]]
local string s=""
loop
exitwhen cur==0
set s=Savecode___itochar(s__BigNum_l_leaf[cur])+s
set cur=s__BigNum_l_next[cur]
endloop
return s
endfunction
function s__Savecode_FromString takes integer this,string s returns nothing
local integer i=StringLength(s)-1
local integer cur=s__BigNum_l_create()
set s__BigNum_list[s__Savecode_bignum[this]]=cur
loop
set s__BigNum_l_leaf[cur]=Savecode___chartoi(SubString(s,i,i+1))
exitwhen i<=0
set s__BigNum_l_next[cur]=s__BigNum_l_create()
set cur=s__BigNum_l_next[cur]
set i=i-1
endloop
endfunction
function s__Savecode_Hash takes integer this returns integer
local integer hash=0
local integer x
local integer cur=s__BigNum_list[s__Savecode_bignum[this]]
loop
exitwhen cur==0
set x=s__BigNum_l_leaf[cur]
set hash=ModuloInteger(hash+79*hash/(x+1)+293*x/(1+hash-(hash/Savecode___BASE())*Savecode___BASE())+479,Savecode___HASHN())
set cur=s__BigNum_l_next[cur]
endloop
return hash
endfunction
function s__Savecode_Obfuscate takes integer this,integer key,integer sign returns nothing
local integer seed=GetRandomInt(0,Savecode___MAXINT())
local integer advance
local integer x
local integer cur=s__BigNum_list[s__Savecode_bignum[this]]
if sign==-1 then
call SetRandomSeed(s__BigNum_LastDigit(s__Savecode_bignum[this]))
set s__BigNum_l_leaf[cur]=Savecode___modb(s__BigNum_l_leaf[cur]+sign*GetRandomInt(0,Savecode___BASE()-1))
set x=s__BigNum_l_leaf[cur]
endif
call SetRandomSeed(key)
loop
exitwhen cur==0
if sign==-1 then
set advance=s__BigNum_l_leaf[cur]
endif
set s__BigNum_l_leaf[cur]=Savecode___modb(s__BigNum_l_leaf[cur]+sign*GetRandomInt(0,Savecode___BASE()-1))
if sign==1 then
set advance=s__BigNum_l_leaf[cur]
endif
set advance=advance+GetRandomInt(0,Savecode___BASE()-1)
call SetRandomSeed(advance)
set x=s__BigNum_l_leaf[cur]
set cur=s__BigNum_l_next[cur]
endloop
if sign==1 then
call SetRandomSeed(x)
set s__BigNum_l_leaf[s__BigNum_list[s__Savecode_bignum[this]]]=Savecode___modb(s__BigNum_l_leaf[s__BigNum_list[s__Savecode_bignum[this]]]+sign*GetRandomInt(0,Savecode___BASE()-1))
endif
call SetRandomSeed(seed)
endfunction
function s__Savecode_Dump takes integer this returns nothing
local integer cur=s__BigNum_list[s__Savecode_bignum[this]]
local string s=""
set s="max: "+R2S(s__Savecode_digits[this])
loop
exitwhen cur==0
set s=I2S(s__BigNum_l_leaf[cur])+" "+s
set cur=s__BigNum_l_next[cur]
endloop
call BJDebugMsg(s)
endfunction
function s__Savecode_Save takes integer this,string p,integer loadtype returns string
local integer key=Savecode___scommhash(p)+loadtype*73
local string s
local integer hash
call s__Savecode_Clean(this)
set hash=s__Savecode_Hash(this)
call s__Savecode_Encode(this,hash,Savecode___HASHN())
call s__Savecode_Clean(this)
call s__Savecode_Pad(this)
call s__Savecode_Obfuscate(this,key,1)
return s__Savecode_ToString(this)
endfunction
function s__Savecode_Load takes integer this,string l__ps,string s,integer loadtype returns boolean
local integer ikey=Savecode___scommhash(l__ps)+loadtype*73
local integer inputhash
call s__Savecode_FromString(this,s)
call s__Savecode_Obfuscate(this,ikey,-1)
set inputhash=s__Savecode_Decode(this,Savecode___HASHN())
call s__Savecode_Clean(this)
return inputhash==s__Savecode_Hash(this)
endfunction
